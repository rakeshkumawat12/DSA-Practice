function leaders(arr, n) {
    let v = [];
    
    let max = arr[n - 1];

    for (let i = n - 1; i >= 0; i--) {
        if (arr[i] >= max) {
            max = arr[i];
            v.push(max);
        }
    }
    v.reverse();
    console.log(v);
}

let arr = [16, 17, 4, 3, 5, 2];
let n = arr.length;
leaders(arr, n)